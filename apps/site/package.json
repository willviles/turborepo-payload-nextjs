{
  "name": "@foo/site",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "pnpm with-env next build",
    "clean": "rm -Rf .next .turbo node_modules dist .tsbuildinfo",
    "dev": "pnpm with-env next dev -p 3000",
    "lint": "SKIP_ENV_VALIDATION=1 next lint",
    "lint:fix": "pnpm lint -- --fix",
    "start": "pnpm with-env next start -p 3000",
    "type-check": "tsc --noEmit",
    "with-env": "dotenv -e ../../.env --"
  },
  "dependencies": {
    "@types/node": "18.16.3",
    "@types/react": "18.2.0",
    "@types/react-dom": "18.2.1",
    "autoprefixer": "10.4.14",
    "dotenv-cli": "^6.0.0",
    "eslint": "8.39.0",
    "eslint-config-next": "13.3.4",
    "next": "13.3.4",
    "postcss": "8.4.23",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tailwindcss": "3.3.2",
    "typescript": "5.0.4"
  }
}
